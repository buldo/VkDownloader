<UserControl x:Class="VkDownloader.Desktop.Views.AuthView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:VkDownloader.Desktop.Views"
             xmlns:userControls="clr-namespace:VkDownloader.Desktop.UserControls"
             xmlns:mvvm="http://prismlibrary.com/"
             mc:Ignorable="d"
             mvvm:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <userControls:VkAuthUserControl ApplicationId="{Binding AppId}" Token="{Binding AccessToken}"/>
    </Grid>
</UserControl>
